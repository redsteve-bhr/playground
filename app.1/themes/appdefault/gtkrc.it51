style "slider"
{

  GtkRange::stepper-size    = 100
  GtkRange::stepper-spacing =   0
  GtkRange::slider-width    =  40

  engine "themeit"
  {
    image 
    {
      function = SLIDER
      file     = "images/flat-normal.png"
      border   = { 1, 2, 1, 2 }
    }
    image 
    {
      function = BOX
      state    = ACTIVE
      file     = "images/slider.png"
      border   = { 18, 18, 18, 18 }
    }
  }
}

