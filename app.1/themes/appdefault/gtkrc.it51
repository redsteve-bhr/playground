
style "default"
{
  font_name      = "Arial 21"
  fg[NORMAL]     = "#000000"
  base[ACTIVE]   = "#657f96"
  base[SELECTED] = "#657f96"
  base[NORMAL]   = "#FFFFFF"

  xthickness =  0
  ythickness =  0

  engine "themeit" 
  {
  }
}

style "default-gtk"
{
  engine ""
  {
  }
}

style "window"
{
  xthickness = 20
  ythickness = 20
  
  engine "themeit"
  {
    bg_pixmap = "images/background.png"
  }
}

style "waitbox-window"
{
  xthickness = 20
  ythickness = 20
  
  engine "themeit"
  {
    bg_pixmap = none
    image 
    {
      function  = FLAT_BOX
      file      = "images/background.png"
      border    = { 0,0,0,0 }
    }
  }  
}

style "title"
{
  xthickness = 16
  ythickness = 16
  
  engine "themeit"
  {
    image 
    {
      function  = BOX
      file      = "images/button-normal.png"
      border    = { 16, 16, 16, 16 }
    }
  }
}

style "title-text"
{
  font_name  = "Arial Bold 25"
}

style "button"
{
  xthickness = 8
  ythickness = 8

  engine "themeit"
  {
    image
    {
      function = BOX
      state    = ACTIVE
      file     = "images/button-down.png"
      border   = { 16, 16, 16, 16 }
    }
    image 
    {
      function = BOX
      file     = "images/button-normal.png"
      border   = { 16, 16, 16, 16 }
    }
  }
}

style "up-button"
{
  xthickness = 8
  ythickness = 8

  engine "themeit"
  {
    image
    {
      function  = BOX
      state     = ACTIVE
      file      = "images/button-active.png"
      border    = { 14, 14, 14,  0 }
      clipping  = {  0,  0,  0, 14 }
    }
    image 
    {
      function  = BOX
      file      = "images/button-normal.png"
      border    = { 14, 14, 14,  0 }
      clipping  = {  0,  0,  0, 14 }
    }
  }
}

